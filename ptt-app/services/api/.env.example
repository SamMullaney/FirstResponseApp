## When running Postgres via docker-compose the container is bound to host port 5433
## (see ptt-app/docker-compose.yml). Update this value if you run Postgres locally.
DATABASE_URL="postgresql://ptt_user:ptt_password@localhost:5433/ptt_db?schema=public"
JWT_SECRET="change-me-to-a-strong-random-secret"
PORT=3001
